{
  "LiveReload": {
    "LiveReloadEnabled": false,
    "ClientFileExtensions": ".cshtml,.css,.js,.htm,.html,.ts,.razor,.custom",
    "ServerRefreshTimeout": 3000,
    "WebSocketUrl": "/__livereload",
    "WebSocketHost": "ws://localhost:5000",
    "FolderToMonitor": "~/"
  },
  "detailedErrors": "true",
  "AppSettings": {
    "SiteUrl": ""
  },
  "SwitchSettings": {
    "EnableRedirectNonWwwToWww": false,
    "EnableRedirectHttpToHttps": true,
    "EnableETags": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [

    ]
  }
}
